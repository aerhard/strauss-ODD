  <define name="att.fragmentable.attribute.part">
    <optional>
      <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" name="part" a:defaultValue="N">
        <a:documentation>specifies whether or not its parent element is fragmented in some way, typically by some other overlapping structure: for example a speech which is divided between two or more verse stanzas, a paragraph which is split across a page division, a verse line which is divided between two speakers.</a:documentation>
        <choice>
          <value>Y</value>
          <a:documentation>the element is fragmented in some (unspecified) respect</a:documentation>
          <value>N</value>
          <a:documentation>either the element is not fragmented, or no claim is made as to its completeness.</a:documentation>
          <value>I</value>
          <a:documentation>this is the initial part of a fragmented element</a:documentation>
          <value>M</value>
          <a:documentation>this is a medial part of a fragmented element</a:documentation>
          <value>F</value>
          <a:documentation>this is the final part of a fragmented element</a:documentation>
        </choice>
      </attribute>
    </optional>
  </define>

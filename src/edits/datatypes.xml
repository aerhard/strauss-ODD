		<macroSpec ident="data.digit">
			<content>
				<rng:data type="string">
					<rng:param name="pattern">\d</rng:param>
				</rng:data>
			</content>
		</macroSpec>

		<macroSpec ident="data.gndIdno">
			<content>
				<rng:data type="string">
					<rng:param name="pattern">\d{1,9}\-?[\dX]</rng:param>
				</rng:data>
			</content>
		</macroSpec>

		<macroSpec ident="data.rsqvIdno">
			<content>
				<rng:data type="string">
					<rng:param name="pattern">[a-z]\d{4,5}</rng:param>
				</rng:data>
			</content>
		</macroSpec>


   <define name="orgName">
      <element name="orgName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(organization name) contains an organizational name. [13.2.2. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.datable.attributes"/>
         <ref name="att.editLike.attributes"/>
         <ref name="att.personal.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="persName">
      <element name="persName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including one or more of the person's forenames, surnames, honorifics, added names, etc. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>  
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="persName-constraint-persNameStartsWithWithespace">
            <rule context="tei:persName">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="not(@type) and matches(.,'^\s')">whitespace at the beginning of element persName</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="persName-constraint-persnameWithoutTextContent">
            <rule context="tei:persName">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           xml:lang="de"
                           test="normalize-space(.) = ''">Whitespace only</sch:report>
            </rule>
         </pattern>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides an externally-defined means of identifying the entity (or entities) being named, using a coded value of some kind.</a:documentation>
               <ref name="key.person.pattern"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="surname">
      <element name="surname">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a family (inherited) name, as opposed to a given, baptismal, or nick name. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
      </element>
   </define>
   <define name="forename">
      <element name="forename">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a forename, given or baptismal name. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.personal.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="genName">
      <element name="genName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(generational name component) contains a name component used to distinguish otherwise similar names on the basis of the relative ages or generations of the persons named. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.personal.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="nameLink">
      <element name="nameLink">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(name link) contains a connecting phrase or link used within a name but not regarded as part of it, such as van der or of. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="addName">
      <element name="addName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(additional namehinzugefügter Name) contains an additional name component, such as a nickname, epithet, or alias, or any other descriptive phrase used within a personal name. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
            <choice>
               <value>nick</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>epithet</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="roleName">
      <element name="roleName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a name component which indicates that the referent has a particular role or position in society, such as an official title or rank. [13.2.1. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.personal.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="placeName">
      <element name="placeName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains an absolute or relative place name. [13.2.3. ]</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="settlement"/>
               <ref name="region"/>
               <ref name="country"/>
               <ref name="hi"/>
               <ref name="geogName"/>
               <ref name="bloc"/>
               <text/>
            </choice>
         </oneOrMore>
         <ref name="att.personal.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">categorisation of varying localities.</a:documentation>
               <choice>
                  <value>theatre</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes buildings designed for performing plays, operas etc. (e.g. (amphi)theatres, operas, ball rooms).</a:documentation>
                  <value>building</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes all sorts of building where no other category is applicable (e.g. bath, bookshop, office).</a:documentation>
                  <value>hospitality</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes buildings designed for lodging, serving food, drinks, etc. (e.g. hotels, bars, inns).</a:documentation>
                  <value>sacral-building</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes buildings serving religious purposes (e.g. churches, abbeys).</a:documentation>
                  <value>administrative-building</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes buildings designed for government administration (e.g. city hall, castle, police station, custom office).</a:documentation>
                  <value>art-building</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes buildings designed for (art) exhibitions (e.g. museums, galleries).</a:documentation>
                  <value>shop</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes buildings designed for selling goods and services to the end-user (e.g. markets, shops).</a:documentation>
                  <value>street</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes a street or a street name.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="bloc">
      <element name="bloc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(bloc) marks the name of a geographical or political unit consisting of two or more nation, states or countries. [13.2.3. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.naming.attributes"/>
         <ref name="att.typed.attributes"/>
         <ref name="att.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="country">
      <element name="country">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(country) marks the name of a geo-political unit, such as a nation or country at the contemporary period. [13.2.3. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.naming.attributes"/>
         <ref name="att.typed.attributes"/>
         <ref name="att.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="region">
      <element name="region">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks the name of an administrative unit at the contemporary period such as a state, province, or county, larger than a settlement, but smaller than a country. NB not some geographical feature which shall be marked up with geogName. [13.2.3. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attributes"/>
         <ref name="att.naming.attributes"/>
         <ref name="att.typed.attributes"/>
         <ref name="att.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="settlement">
      <element name="settlement">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks the name of a settlement identified as a single geo-political or administrative unit. [13.2.3. ]</a:documentation>
         <ref name="macro.phraseSeq"/>  
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="settlement-constraint-settlementStartsWithWithespace">
            <rule context="tei:settlement">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           xml:lang="de"
                           test="matches(.,'^\s')">whitespace at the beginning of element settlement</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="settlement-constraint-settlementWithoutTextContent">
            <rule context="tei:settlement">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           xml:lang="de"
                           test="normalize-space(.) = ''">Whitespace only</sch:report>
            </rule>
         </pattern>
      </element>
   </define>
   <define name="geogName">
      <element name="geogName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(geographical name) marks a name associated with some geographical feature (e.g. seas, mountains). NB not some administrative unit. [13.2.3. ]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.datable.attributes"/>
         <ref name="att.editLike.attributes"/>
         <ref name="att.global.attributes"/>
         <ref name="att.naming.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="affiliation">
      <element name="affiliation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(affiliationMitgliedschaft) contains an informal description of a person's present or past affiliation with some organization, for example an employer or sponsor.enthält eine formlose Beschreibung der momenanten oder vergangenen Mitgliedschaft einer Person in einer Organisation, beispielsweise ein Arbeitgeber oder Förderer. [15.2.2. ]</a:documentation>
         <choice>
            <interleave>
               <optional>
                  <text/>
               </optional>
               <optional>
                  <ref name="note"/>
               </optional>
            </interleave>
            <group>
               <ref name="orgName"/>
               <optional>
                  <ref name="address"/>
               </optional>
            </group>
         </choice>
         <ref name="att.datable.w3c.attributes"/>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides an externally-defined means of identifying the entity (or entities) being named, using a coded value of some kind.</a:documentation>
               <choice>
                  <value>quartetMember</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a member ot the quartet.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="birth">
      <element name="birth">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(birthGeburt) contains information about a person's birth, such as its date and place. [15.2.2. ]</a:documentation>
         <zeroOrMore>
            <ref name="date"/>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="placeName"/>
         </zeroOrMore>
      </element>
   </define>
   <define name="death">
      <element name="death">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(deathTod) contains information about a person's death, such as its date and place.Enthält Informationen über den Tod einer Person, wie etwa das Datum und der Ort. [15.2.2. ]</a:documentation>
         <zeroOrMore>
            <ref name="date"/>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="placeName"/>
         </zeroOrMore>
      </element>
   </define>
   <define name="event">
      <element name="event">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(eventEreignis) contains data relating to any kind of significant event associated with a person, place, or organization.enthält Daten, die zu irgendeinem signifkanten Ereignis, das mit eienr Person, einem Ort oder eine Organisation assoziiert werden kann, zugeordnet werden. [13.3.1. ]</a:documentation>
         <ref name="head"/>
         <choice>
            <ref name="p"/>
            <ref name="list"/>
         </choice>
         <interleave>
            <zeroOrMore>
               <ref name="p"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="list"/>
            </zeroOrMore>
         </interleave>
         <ref name="att.datable.w3c.attributes"/>
         <ref name="att.sortable.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>studentOf</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">student.</a:documentation>
                  <value>profOf</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">prof.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="status">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">describes the status of a document either currently or, when associated with a dated element, at the time indicated.</a:documentation>
               <choice>
                  <value>restricted</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="where">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the location of an event by pointing to a place element</a:documentation>
               <ref name="data.pointer"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="occupation">
      <element name="occupation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains an informal description of a person's trade, profession or occupation. [15.2.2. ]</a:documentation>
         <data type="string">
            <param name="minLength">3</param>
         </data>
         <ref name="att.datable.w3c.attributes"/>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="person">
      <element name="person">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides information about an identifiable individual, for example a participant in a language interaction, or a person referred to in a historical source. [13.3.2.  15.2.2. ]</a:documentation>
         <choice>
            <element name="ref">
               <attribute name="type">
                  <choice>
                     <value>duplicate</value>
                     <value>deletion</value>
                  </choice>
               </attribute>
               <attribute name="target">
                  <ref name="key.person.pattern"/>
               </attribute>
               <empty/>
            </element>
            <group>
               <zeroOrMore>
                  <ref name="idno"/>
               </zeroOrMore>
               <element name="persName">
                  <ref name="macro.persNameContent"/>
                  <attribute name="type">
                     <value>reg</value>
                  </attribute>
                  <optional>
                     <attribute name="xml:lang">
                        <ref name="data.language"/>
                     </attribute>
                  </optional>
               </element>
               <optional>
                  <element name="persName">
                     <ref name="macro.persNameContent"/>
                     <attribute name="type">
                        <value>full</value>
                     </attribute>
                     <optional>
                        <attribute name="xml:lang">
                           <ref name="data.language"/>
                        </attribute>
                     </optional>
                  </element>
               </optional>
               <optional>
                  <element name="persName">
                     <ref name="macro.persNameContent"/>
                     <attribute name="type">
                        <value>real</value>
                     </attribute>
                     <optional>
                        <attribute name="xml:lang">
                           <ref name="data.language"/>
                        </attribute>
                     </optional>
                  </element>
               </optional>
               <zeroOrMore>
                  <element name="persName">
                     <ref name="macro.persNameContent"/>
                     <attribute name="type">
                        <choice>
                           <value>alt</value>
                           <value>pseud</value>
                        </choice>
                     </attribute>
                     <optional>
                        <attribute name="xml:lang">
                           <ref name="data.language"/>
                        </attribute>
                     </optional>
                     <optional>
                        <attribute name="from">
                           <choice>
                              <data type="date"/>
                              <data type="gYear"/>
                           </choice>
                        </attribute>
                     </optional>
                     <optional>
                        <attribute name="to">
                           <choice>
                              <data type="date"/>
                              <data type="gYear"/>
                           </choice>
                        </attribute>
                     </optional>
                     <optional>
                        <attribute name="subtype">
                           <choice>
                              <value>birth</value>
                              <value>married</value>
                              <value>role</value>
                           </choice>
                        </attribute>
                     </optional>
                  </element>
               </zeroOrMore>
               <interleave>
                  <optional>
                     <ref name="birth"/>
                  </optional>
                  <optional>
                     <ref name="death"/>
                  </optional>
                  <ref name="sex"/>
                  <zeroOrMore>
                     <ref name="occupation"/>
                  </zeroOrMore>
                  <zeroOrMore>
                     <ref name="residence"/>
                  </zeroOrMore>
                  <zeroOrMore>
                     <ref name="affiliation"/>
                  </zeroOrMore>
                  <optional>
                     <element name="note">
                        <ref name="macro.specialPara"/>
                        <attribute name="type">
                           <value>bioSummary</value>
                        </attribute>
                     </element>
                  </optional>
                  <zeroOrMore>
                     <ref name="event"/>
                  </zeroOrMore>
                  <optional>
                     <element name="figure">
                        <ref name="head"/>
                        <attribute name="n">
                           <value>reg</value>
                        </attribute>
                     </element>
                     <zeroOrMore>
                        <element name="figure">
                           <ref name="head"/>
                        </element>
                     </zeroOrMore>
                  </optional>
               </interleave>
            </group>
         </choice> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="person-constraint-personHeuristics1">
            <rule context="tei:person">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="count(./tei:persName[@subtype='birth']) &gt; 1">Mehrere Geburtsnamen gefunden</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="person-constraint-personHeuristics2">
            <rule context="tei:person">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="./tei:birth/tei:date/(@when, @notBefore, @notAfter) &gt; ./tei:death/tei:date/(@when, @notBefore, @notAfter) and not(./tei:death/tei:date/(@when, @notBefore, @notAfter)[starts-with(., '-')])">Geburtsdatum nach Todesdatum</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="person-constraint-personHeuristics3">
            <rule context="tei:person">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="./tei:death/tei:date/(@when, @notBefore, @notAfter) &gt; ./tei:birth/tei:date/(@when, @notBefore, @notAfter) and ./tei:death/tei:date/(@when, @notBefore, @notAfter)[starts-with(., '-')]">Geburtsdatum nach Todesdatum</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="person-constraint-personHeuristics4">
            <rule context="tei:person">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test=".//(tei:persName, tei:workName, tei:surname, tei:forename, tei:hi, tei:rs, tei:event, tei:note)[normalize-space(.) = '']">Leeres Element gefunden</sch:report>
            </rule>
         </pattern>
         <ns xmlns="http://purl.oclc.org/dsdl/schematron"
             prefix="tei"
             uri="http://www.tei-c.org/ns/1.0"/>
         <ns xmlns="http://purl.oclc.org/dsdl/schematron"
             prefix="rng"
             uri="http://relaxng.org/ns/structure/1.0"/>
         <ref name="att.sortable.attributes"/>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="status"
                    a:defaultValue="draft">
            <a:documentation>describes the status of a document either currently or, when associated with a dated element, at the time indicated.
Sample values include: 1] approved; 2] candidate; 3] cleared; 4] deprecated; 5] draft; 6] embargoed; 7] expired; 8] frozen; 9] galley; 10] proposed; 11] published; 12] recommendation; 13] submitted; 14] unfinished; 15] withdrawn</a:documentation>
            <ref name="data.enumerated"/>
         </attribute>
         <attribute name="source">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a list of one or more pointers indicating sources supporting the given intervention or interpretation.enthält eine Liste von einem oder mehreren Pointer, die die vorliegende Intervention oder Interpretation einer Quelle unterstützen.</a:documentation>
            <choice>
               <value>WeGA</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">persons from the database of the Carl-Maria-von-Weber-Gesamtausgabe (WeGA).</a:documentation>
               <value>SchEnd</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">B. Appel: Schumann in Endenich.</a:documentation>
               <value>MB</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Meyerbeer letter edition</a:documentation>
               <value>BGA</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">letters of Beethoven</a:documentation>
               <value>LoB</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Lortzing letter edition</a:documentation>
               <value>HoB</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">E.T.A. Hoffmann letter edition</a:documentation>
               <value>SchTb</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">diary of Schumann</a:documentation>
               <value>Bach</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">U. Wolf: collection of names from the Bach institute</a:documentation>
               <value>Wies</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chr. Wiesenfeldt: names of 700 cellists from the 19th century</a:documentation>
            </choice>
         </attribute>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
            <ref name="key.person.pattern"/>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="place">
      <element name="place">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains data about a geographic location [13.3.4. ]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="model.headLike"/>
            </zeroOrMore>
            <choice>
               <group>
                  <zeroOrMore>
                     <ref name="model.pLike"/>
                  </zeroOrMore>
               </group>
               <zeroOrMore>
                  <choice>
                     <ref name="model.labelLike"/>
                     <ref name="model.placeStateLike"/>
                     <ref name="model.placeEventLike"/>
                  </choice>
               </zeroOrMore>
            </choice>
            <zeroOrMore>
               <choice>
                  <ref name="model.noteLike"/>
                  <ref name="model.biblLike"/>
                  <ref name="idno"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <choice>
                  <ref name="model.placeLike"/>
               </choice>
            </zeroOrMore>
         </group>
         <ref name="att.global.attributes"/>
         <ref name="att.typed.attributes"/>
         <ref name="att.editLike.attributes"/>
         <ref name="att.sortable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="residence">
      <element name="residence">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(residenceAufenthaltsort) describes a person's present or past places of residence.beschreibt den momentanen oder vergangene Aufenthaltsort(e) einer Person. [15.2.2. ]</a:documentation>
         <choice>
            <ref name="settlement"/>
            <ref name="region"/>
            <ref name="country"/>
         </choice>
         <ref name="att.datable.w3c.attributes"/>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="sex">
      <element name="sex">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the sex of a person. [13.3.2.1. ]</a:documentation>
         <choice>
            <value>f</value>
            <value>m</value>
            <value>unknown</value>
         </choice>
         <optional>
            <attribute name="value">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies a coded value for sex</a:documentation>
               <list>
                  <oneOrMore>
                     <ref name="data.sex"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="characterName">
      <element name="characterName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(characterName) name of a role or character in a work.</a:documentation>
         <ref name="macro.phraseSeq"/>  
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="characterName-constraint-characterNameStartsWithWithespace">
            <rule context="tei:characterName">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="matches(.,'^\s')">whitespace at the beginning of element characterName</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="characterName-constraint-characternameWithoutTextContent">
            <rule context="tei:characterName">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           xml:lang="de"
                           test="normalize-space(.) = ''">Whitespace only</sch:report>
            </rule>
         </pattern>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="workName">
      <element name="workName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">name of a work (e.g. composition)</a:documentation>
         <ref name="macro.phraseSeq"/>  
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="workName-constraint-workNameStartsWithWithespace">
            <rule context="tei:workName">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="matches(.,'^\s')">whitespace at the beginning of element workName</sch:report>
            </rule>
         </pattern> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="workName-constraint-worknameWithoutTextContent">
            <rule context="tei:workName">
               <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           xml:lang="de"
                           test="normalize-space(.) = ''">Whitespace only</sch:report>
            </rule>
         </pattern>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides an externally-defined means of identifying the entity (or entities) being named, using a coded value of some kind.</a:documentation>
               <choice>
                  <ref name="key.work.pattern"/>
                  <ref name="key.works.pattern"/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <ref name="data.certainty"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="footNote">
      <element name="footNote">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Special element for the transcription of a footnote (not necessarily to be rendered as such)</a:documentation>
         <ref name="macro.specialPara"/>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
            <data type="ID"/>
         </attribute>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="person"/>
      </choice>
   </start>
</grammar>

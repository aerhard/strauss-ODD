  <define name="person">
    <element name="person">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides information about an identifiable individual, for example a participant in a language interaction, or a person referred to in a historical source. [13.3.2.  15.2.2. ]</a:documentation>
      <choice>
        <element name="ref">
          <attribute name="type">
            <choice>
              <value>duplicate</value>
              <value>deletion</value>
            </choice>
          </attribute>
          <attribute name="target">
            <ref name="key.person.pattern"/>
          </attribute>
          <empty/>
        </element>
        <group>
          <zeroOrMore>
            <ref name="idno"/>
          </zeroOrMore>
          <element name="persName">
            <ref name="macro.persNameContent"/>
            <attribute name="type">
              <value>reg</value>
            </attribute>
            <optional>
              <attribute name="xml:lang">
                <ref name="data.language"/>
              </attribute>
            </optional>
          </element>
          <optional>
            <element name="persName">
              <ref name="macro.persNameContent"/>
              <attribute name="type">
                <value>full</value>
              </attribute>
              <optional>
                <attribute name="xml:lang">
                  <ref name="data.language"/>
                </attribute>
              </optional>
            </element>
          </optional>
          <optional>
            <element name="persName">
              <ref name="macro.persNameContent"/>
              <attribute name="type">
                <value>real</value>
              </attribute>
              <optional>
                <attribute name="xml:lang">
                  <ref name="data.language"/>
                </attribute>
              </optional>
            </element>
          </optional>
          <zeroOrMore>
            <element name="persName">
              <ref name="macro.persNameContent"/>
              <attribute name="type">
                <choice>
                  <value>alt</value>
                  <value>pseud</value>
                </choice>
              </attribute>
              <optional>
                <attribute name="xml:lang">
                  <ref name="data.language"/>
                </attribute>
              </optional>
              <optional>
                <attribute name="from">
                  <choice>
                    <data type="date"/>
                    <data type="gYear"/>
                  </choice>
                </attribute>
              </optional>
              <optional>
                <attribute name="to">
                  <choice>
                    <data type="date"/>
                    <data type="gYear"/>
                  </choice>
                </attribute>
              </optional>
              <optional>
                <attribute name="subtype">
                  <choice>
                    <value>birth</value>
                    <value>married</value>
                    <value>role</value>
                  </choice>
                </attribute>
              </optional>
            </element>
          </zeroOrMore>
          <interleave>
            <optional>
              <ref name="birth"/>
            </optional>
            <optional>
              <ref name="death"/>
            </optional>
            <ref name="sex"/>
            <zeroOrMore>
              <ref name="occupation"/>
            </zeroOrMore>
            <zeroOrMore>
              <ref name="residence"/>
            </zeroOrMore>
            <zeroOrMore>
              <ref name="affiliation"/>
            </zeroOrMore>
            <optional>
              <element name="note">
                <ref name="macro.specialPara"/>
                <attribute name="type">
                  <value>bioSummary</value>
                </attribute>
              </element>
            </optional>
            <zeroOrMore>
              <ref name="event"/>
            </zeroOrMore>
            <optional>
              <element name="figure">
                <ref name="head"/>
                <attribute name="n">
                  <value>reg</value>
                </attribute>
              </element>
              <zeroOrMore>
                <element name="figure">
                  <ref name="head"/>
                </element>
              </zeroOrMore>
            </optional>
          </interleave>
        </group>
      </choice>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="person-constraint-personHeuristics1">
        <rule context="tei:person">
          <sch:report xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" test="./tei:birth/tei:date/(@when, @notBefore, @notAfter) &gt; ./tei:death/tei:date/(@when, @notBefore, @notAfter) and not(./tei:death/tei:date/(@when, @notBefore, @notAfter)[starts-with(., '-')])">Geburtsdatum nach Todesdatum</sch:report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="person-constraint-personHeuristics2">
        <rule context="tei:person">
          <sch:report xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" test="./tei:death/tei:date/(@when, @notBefore, @notAfter) &gt; ./tei:birth/tei:date/(@when, @notBefore, @notAfter) and ./tei:death/tei:date/(@when, @notBefore, @notAfter)[starts-with(., '-')]">Geburtsdatum nach Todesdatum</sch:report>
        </rule>
      </pattern>
      <ref name="att.docStatus.attributes"/>
      <attribute name="source">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a list of one or more pointers indicating sources supporting the given intervention or interpretation.enthält eine Liste von einem oder mehreren Pointer, die die vorliegende Intervention oder Interpretation einer Quelle unterstützen.</a:documentation>
        <choice>
          <value>WeGA</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">persons from the database of the Carl-Maria-von-Weber-Gesamtausgabe (WeGA).</a:documentation>
          <value>SchEnd</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">B. Appel: Schumann in Endenich.</a:documentation>
          <value>MB</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Meyerbeer letter edition</a:documentation>
          <value>BGA</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">letters of Beethoven</a:documentation>
          <value>LoB</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Lortzing letter edition</a:documentation>
          <value>HoB</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">E.T.A. Hoffmann letter edition</a:documentation>
          <value>SchTb</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">diary of Schumann</a:documentation>
          <value>Bach</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">U. Wolf: collection of names from the Bach institute</a:documentation>
          <value>Wies</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chr. Wiesenfeldt: names of 700 cellists from the 19th century</a:documentation>
        </choice>
      </attribute>
      <attribute name="xml:id">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
        <ref name="key.person.pattern"/>
      </attribute>
      <empty/>
    </element>
  </define>
  <define name="residence">
    <element name="residence">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(residenceAufenthaltsort) describes a person's present or past places of residence.beschreibt den momentanen oder vergangene Aufenthaltsort(e) einer Person. [15.2.2. ]</a:documentation>
      <choice>
        <ref name="settlement"/>
        <ref name="region"/>
        <ref name="country"/>
      </choice>
      <ref name="att.datable.w3c.attributes"/>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="sex">
    <element name="sex">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the sex of a person. [13.3.2.1. ]</a:documentation>
      <choice>
        <value>f</value>
        <value>m</value>
        <value>unknown</value>
      </choice>
      <optional>
        <attribute name="value">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies a coded value for sex</a:documentation>
          <list>
            <oneOrMore>
              <ref name="data.sex"/>
            </oneOrMore>
          </list>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="characterName">
    <element name="characterName">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(characterName) name of a role or character in a work.</a:documentation>
      <ref name="macro.phraseSeq"/>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="workName">
    <element name="workName">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">name of a work (e.g. composition)</a:documentation>
      <ref name="macro.phraseSeq"/>
      <optional>
        <attribute name="key">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides an externally-defined means of identifying the entity (or entities) being named, using a coded value of some kind.</a:documentation>
          <choice>
            <ref name="key.work.pattern"/>
            <ref name="key.works.pattern"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="footNote">
    <element name="footNote">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Special element for the transcription of a footnote (not necessarily to be rendered as such)</a:documentation>
      <ref name="macro.specialPara"/>
      <attribute name="xml:id">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
        <data type="ID"/>
      </attribute>
      <empty/>
    </element>
  </define>
  <start>
    <choice>
      <ref name="person"/>
    </choice>
  </start>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="wegaPersons-constraint-whitespaceOnlyContent">
    <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="tei:surname | tei:forename | tei:persName | tei:author[not(@sameAs)] | tei:occupation | tei:settlement | tei:country | tei:rs | tei:workName | tei:characterName | tei:hi[not(.//tei:gap)] | tei:head | tei:note[not(@type='part')] | tei:placeName | tei:title">
      <sch:report test="normalize-space(.) = ''"><sch:name/> has whitespace only content</sch:report>
    </sch:rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="wegaPersons-constraint-elementStartsWithWhitespace">
    <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="tei:surname | tei:forename | tei:persName[not(@type)] | tei:author | tei:occupation | tei:settlement | tei:country | tei:rs | tei:workName | tei:characterName | tei:hi | tei:placeName[not(parent::tei:address or parent::tei:birth or parent::tei:death)]">
      <sch:report test="matches(.,'^\s')">whitespace at the beginning of element <sch:name/>
         </sch:report>
    </sch:rule>
  </pattern>
</grammar>

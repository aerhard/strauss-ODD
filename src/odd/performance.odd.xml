				<moduleRef key="drama" include="actor castItem castList performance role"/>
				
				<specGrpRef target="#edits.classes.att"/>
				<specGrpRef target="#edits.datatypes"/>
				<specGrpRef target="#edits.elements"/>
				<specGrpRef target="#edits.macros"/>

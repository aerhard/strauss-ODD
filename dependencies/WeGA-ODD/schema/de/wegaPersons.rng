  <define name="region">
    <element name="region">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">markiert den Namen einer Verwaltungseinheit der zeitgenössischen Periode wie etwa ein Staat, eine Provinz, Landkreis, größer als eine Siedlung, aber kleiner als ein Land. NB nicht eine geographische Region, die mit geogName ausgezeichnet werden soll. [13.2.3. ]</a:documentation>
      <ref name="macro.phraseSeq"/>
      <ref name="att.global.attributes"/>
      <ref name="att.naming.attributes"/>
      <ref name="att.typed.attributes"/>
      <ref name="att.datable.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="settlement">
    <element name="settlement">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">markiert den Namen einer Stadt, die als einzelne geopolitische oder administrative Einheit identifiziert werden kann. [13.2.3. ]</a:documentation>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="geogName">
    <element name="geogName">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">markiert einen Namen, mit dem eine geographische Region assoziiert wird (z.B. Seen, Gebirge, aber keine Verwaltungseinheiten). [13.2.3. ]</a:documentation>
      <ref name="macro.phraseSeq"/>
      <ref name="att.datable.attributes"/>
      <ref name="att.editLike.attributes"/>
      <ref name="att.global.attributes"/>
      <ref name="att.naming.attributes"/>
      <ref name="att.typed.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="affiliation">
    <element name="affiliation">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> [15.2.2. ]</a:documentation>
      <choice>
        <interleave>
          <optional>
            <text/>
          </optional>
          <optional>
            <ref name="note"/>
          </optional>
        </interleave>
        <group>
          <ref name="orgName"/>
          <optional>
            <ref name="address"/>
          </optional>
        </group>
      </choice>
      <ref name="att.datable.w3c.attributes"/>
      <optional>
        <attribute name="key">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">bietet mittels eines codierten Wertes in einer beliebigen Form ein extern definiertes Mittel zur Identifizierung einer Einheit (oder mehrerer Einheiten).</a:documentation>
          <choice>
            <value>quartetMember</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Mitglied eines Quartets.</a:documentation>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sicherheit) besagt den Grad der Sicherheit, mit der die Intervention oder Interpretation verbunden ist.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="birth">
    <element name="birth">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">enthält Informationen über die Geburt einer Person, wie etwa das Datum und der Ort. [15.2.2. ]</a:documentation>
      <zeroOrMore>
        <ref name="date"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="placeName"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="death">
    <element name="death">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> [15.2.2. ]</a:documentation>
      <zeroOrMore>
        <ref name="date"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="placeName"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="event">
    <element name="event">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> [13.3.1. ]</a:documentation>
      <ref name="head"/>
      <choice>
        <ref name="p"/>
        <ref name="list"/>
      </choice>
      <interleave>
        <zeroOrMore>
          <ref name="p"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="list"/>
        </zeroOrMore>
      </interleave>
      <ref name="att.datable.w3c.attributes"/>
      <optional>
        <attribute name="type">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">charakterisiert das Element in einem gewissen Sinne, wobei irgendeine zweckmäßige Systematik oder Typologie angwendet wird.</a:documentation>
          <choice>
            <value>studentOf</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Schüler.</a:documentation>
            <value>profOf</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Prof.</a:documentation>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="status">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">beschreibt entweder den aktuellen Status eines Dokuments oder den Zeitpunkt zu dem ein veraltetes Element zugeordnet wurde.</a:documentation>
          <choice>
            <value>restricted</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sicherheit) besagt den Grad der Sicherheit, mit der die Intervention oder Interpretation verbunden ist.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="where">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <ref name="data.pointer"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="occupation">
    <element name="occupation">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">enthält eine formlose Beschreibung des Handwerks, des Berufs oder der Beschäftigung einer Person. [15.2.2. ]</a:documentation>
      <ref name="macro.phraseSeq"/>
      <ref name="att.datable.w3c.attributes"/>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sicherheit) besagt den Grad der Sicherheit, mit der die Intervention oder Interpretation verbunden ist.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="person">
    <element name="person">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">liefert Informationen über ein identifizierbares Individuum, beispielsweise ein Teilnehmer einer Sprachinteraktion, oder eine Person, auf die in einer historischen Quelle verwiesen wird. [13.3.2.  15.2.2. ]</a:documentation>
      <choice>
        <element name="ref">
          <attribute name="type">
            <choice>
              <value>duplicate</value>
              <value>deletion</value>
            </choice>
          </attribute>
          <attribute name="target">
            <ref name="key.person.pattern"/>
          </attribute>
          <empty/>
        </element>
        <group>
          <zeroOrMore>
            <ref name="idno"/>
          </zeroOrMore>
          <element name="persName">
            <ref name="macro.persNameContent"/>
            <attribute name="type">
              <value>reg</value>
            </attribute>
            <optional>
              <attribute name="xml:lang">
                <ref name="data.language"/>
              </attribute>
            </optional>
          </element>
          <optional>
            <element name="persName">
              <ref name="macro.persNameContent"/>
              <attribute name="type">
                <value>full</value>
              </attribute>
              <optional>
                <attribute name="xml:lang">
                  <ref name="data.language"/>
                </attribute>
              </optional>
            </element>
          </optional>
          <optional>
            <element name="persName">
              <ref name="macro.persNameContent"/>
              <attribute name="type">
                <value>real</value>
              </attribute>
              <optional>
                <attribute name="xml:lang">
                  <ref name="data.language"/>
                </attribute>
              </optional>
            </element>
          </optional>
          <zeroOrMore>
            <element name="persName">
              <ref name="macro.persNameContent"/>
              <attribute name="type">
                <choice>
                  <value>alt</value>
                  <value>pseud</value>
                </choice>
              </attribute>
              <optional>
                <attribute name="xml:lang">
                  <ref name="data.language"/>
                </attribute>
              </optional>
              <optional>
                <attribute name="from">
                  <choice>
                    <data type="date"/>
                    <data type="gYear"/>
                  </choice>
                </attribute>
              </optional>
              <optional>
                <attribute name="to">
                  <choice>
                    <data type="date"/>
                    <data type="gYear"/>
                  </choice>
                </attribute>
              </optional>
              <optional>
                <attribute name="subtype">
                  <choice>
                    <value>birth</value>
                    <value>married</value>
                    <value>role</value>
                  </choice>
                </attribute>
              </optional>
            </element>
          </zeroOrMore>
          <interleave>
            <optional>
              <ref name="birth"/>
            </optional>
            <optional>
              <ref name="death"/>
            </optional>
            <ref name="sex"/>
            <zeroOrMore>
              <ref name="occupation"/>
            </zeroOrMore>
            <zeroOrMore>
              <ref name="residence"/>
            </zeroOrMore>
            <zeroOrMore>
              <ref name="affiliation"/>
            </zeroOrMore>
            <optional>
              <element name="note">
                <ref name="macro.specialPara"/>
                <attribute name="type">
                  <value>bioSummary</value>
                </attribute>
              </element>
            </optional>
            <zeroOrMore>
              <ref name="event"/>
            </zeroOrMore>
            <optional>
              <element name="figure">
                <ref name="head"/>
                <attribute name="n">
                  <value>reg</value>
                </attribute>
              </element>
              <zeroOrMore>
                <element name="figure">
                  <ref name="head"/>
                </element>
              </zeroOrMore>
            </optional>
          </interleave>
        </group>
      </choice>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="person-constraint-personHeuristics1">
        <rule context="tei:person">
          <sch:report xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" test="./tei:birth/tei:date/(@when, @notBefore, @notAfter) &gt; ./tei:death/tei:date/(@when, @notBefore, @notAfter) and not(./tei:death/tei:date/(@when, @notBefore, @notAfter)[starts-with(., '-')])">Geburtsdatum nach Todesdatum</sch:report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="person-constraint-personHeuristics2">
        <rule context="tei:person">
          <sch:report xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" test="./tei:death/tei:date/(@when, @notBefore, @notAfter) &gt; ./tei:birth/tei:date/(@when, @notBefore, @notAfter) and ./tei:death/tei:date/(@when, @notBefore, @notAfter)[starts-with(., '-')]">Geburtsdatum nach Todesdatum</sch:report>
        </rule>
      </pattern>
      <ref name="att.docStatus.attributes"/>
      <attribute name="source">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
        <choice>
          <value>WeGA</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Personen aus dem Datenbestand der Carl-Maria-von-Weber-Gesamtausgabe (WeGA).</a:documentation>
          <value>SchEnd</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">B. Appel: Schumann in Endenich.</a:documentation>
          <value>MB</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Meyerbeer Briefausgabe</a:documentation>
          <value>BGA</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Briefe Beethovens</a:documentation>
          <value>LoB</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Lortzing Briefausgabe</a:documentation>
          <value>HoB</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">E.T.A. Hoffmann-Briefausgabe</a:documentation>
          <value>SchTb</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Schumanns Tagebuch</a:documentation>
          <value>Bach</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">U. Wolf: Namenssammlung des Bach-Instituts</a:documentation>
          <value>Wies</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chr. Wiesenfeldt: Namen von 700 Violoncellisten des 19. Jahrhunderts</a:documentation>
        </choice>
      </attribute>
      <attribute name="xml:id">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Bezeichner) liefert einen eindeutigen Bezeichner für das Element mit dem Attribut.</a:documentation>
        <ref name="key.person.pattern"/>
      </attribute>
      <empty/>
    </element>
  </define>
  <define name="residence">
    <element name="residence">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> [15.2.2. ]</a:documentation>
      <choice>
        <ref name="settlement"/>
        <ref name="region"/>
        <ref name="country"/>
      </choice>
      <ref name="att.datable.w3c.attributes"/>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sicherheit) besagt den Grad der Sicherheit, mit der die Intervention oder Interpretation verbunden ist.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="sex">
    <element name="sex">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gibt das Geschlecht einer Person an. [13.3.2.1. ]</a:documentation>
      <choice>
        <value>f</value>
        <value>m</value>
        <value>unknown</value>
      </choice>
      <optional>
        <attribute name="value">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies a coded value for sex</a:documentation>
          <list>
            <oneOrMore>
              <ref name="data.sex"/>
            </oneOrMore>
          </list>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="characterName">
    <element name="characterName">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Name eines Charakters) Name einer Rolle oder eines Charakters in einem Werk.</a:documentation>
      <ref name="macro.phraseSeq"/>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sicherheit) besagt den Grad der Sicherheit, mit der die Intervention oder Interpretation verbunden ist.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="workName">
    <element name="workName">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Name des Werkes (z.B. Komposition)</a:documentation>
      <ref name="macro.phraseSeq"/>
      <optional>
        <attribute name="key">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">bietet mittels eines codierten Wertes in einer beliebigen Form ein extern definiertes Mittel zur Identifizierung einer Einheit (oder mehrerer Einheiten).</a:documentation>
          <choice>
            <ref name="key.work.pattern"/>
            <ref name="key.works.pattern"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="cert">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sicherheit) besagt den Grad der Sicherheit, mit der die Intervention oder Interpretation verbunden ist.</a:documentation>
          <ref name="data.certainty"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="footNote">
    <element name="footNote">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Spezielles Element für die Transkription einer Fußnote (die nicht unbedingt als solche dargestellt werden)</a:documentation>
      <ref name="macro.specialPara"/>
      <attribute name="xml:id">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Bezeichner) liefert einen eindeutigen Bezeichner für das Element, dass das Attribut trägt.</a:documentation>
        <data type="ID"/>
      </attribute>
      <empty/>
    </element>
  </define>
  <start>
    <choice>
      <ref name="person"/>
    </choice>
  </start>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="wegaPersons-constraint-whitespaceOnlyContent">
    <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="tei:surname | tei:forename | tei:persName | tei:author[not(@sameAs)] | tei:occupation | tei:settlement | tei:country | tei:rs | tei:workName | tei:characterName | tei:hi[not(.//tei:gap)] | tei:head | tei:note[not(@type='part')] | tei:placeName | tei:title">
      <sch:report test="normalize-space(.) = ''"><sch:name/> has whitespace only content</sch:report>
    </sch:rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="wegaPersons-constraint-elementStartsWithWhitespace">
    <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="tei:surname | tei:forename | tei:persName[not(@type)] | tei:author | tei:occupation | tei:settlement | tei:country | tei:rs | tei:workName | tei:characterName | tei:hi | tei:placeName[not(parent::tei:address or parent::tei:birth or parent::tei:death)]">
      <sch:report test="matches(.,'^\s')">whitespace at the beginning of element <sch:name/>
         </sch:report>
    </sch:rule>
  </pattern>
</grammar>

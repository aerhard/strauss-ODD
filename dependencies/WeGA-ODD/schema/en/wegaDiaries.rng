            <value>rh</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">thaler</a:documentation>
            <value>pf</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">penny</a:documentation>
            <value>f</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gulden</a:documentation>
            <value>xr</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">kreutzer</a:documentation>
            <value>fd</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Friedrich d'or</a:documentation>
